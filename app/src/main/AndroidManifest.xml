<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="Friquiz"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApplication"
        tools:targetApi="31">
        <activity
            android:name=".DB.Partida"
            android:exported="false" />
        <activity
            android:name=".DB.DbPartidas"
            android:exported="false" />
        <activity
            android:name=".DB.DbHelper"
            android:exported="false" />
        <activity
            android:name=".Partidas.AdapterPartidas"
            android:exported="false" />
        <activity
            android:name=".Partidas.PartidasActivity"
            android:exported="false" />
        <activity
            android:name=".Partidas.NombreActivity"
            android:exported="false" />
        <activity
            android:name=".Game.ImageActivity"
            android:exported="false" />
        <activity
            android:name=".Game.EndActivity"
            android:exported="false"
            android:label="@string/title_activity_end"
            android:theme="@style/Theme.MyApplication" />
        <activity
            android:name=".Game.JuegoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Game.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>